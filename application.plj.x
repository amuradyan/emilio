( ;; Emilio configs
  port <PORT> ;; Port to run the server on
  api-key <API_KEY> ;; OpenAI API key
  assistant-id <ASSISTANT_ID> ;; Assistant ID you want to interact with
  thread-id <THREAD_ID>  ;; Thread ID with the assistant you want to interact with
  telegram-bot-token <TELEGRAM_BOT_TOKEN> ;; The token you got from BotFather for your bot
  owners (<TELEGRAM_USER_ID>, <TELEGRAM_USER_ID>, ...) ;; Users, who are allowed to talk to Emilio
)
